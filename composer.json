{
    "name": "rubium/telescope-plugin",
    "description": "Laravel Telescope integration for OctoberCMS.",
    "homepage": "https://github.com/rubium-web/telescope-plugin",
    "type": "october-plugin",
    "license": "proprietary",
    "keywords": ["october", "telescope"],
    "authors": [
        {
            "name": "Rubium Web",
            "email": "radist@rubium.ru",
            "role": "Creator"
        }
    ],
    "require": {
        "laravel/telescope": "4.*"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "archive": {
        "exclude": [
            ".github",
            ".gitignore",
            ".idea/",
            ".gitlab-ci.yml"
        ]
    }
}
